<p id="notice"><%= notice %></p>

<h1>Example Records</h1>

<table>
  <thead>
    <tr>
      <th>Foo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @example_records.each do |example_record| %>
      <tr>
        <td><%= example_record.foo %></td>
        <td><%= link_to 'Show', example_record %></td>
        <td><%= link_to 'Edit', edit_example_record_path(example_record) %></td>
        <td><%= link_to 'Destroy', example_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Example Record', new_example_record_path %>
